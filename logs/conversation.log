def est_premier(n):
  """
  Fonction pour vérifier si un nombre est premier.

  Args:
    n: Le nombre à vérifier.

  Returns:
    True si le nombre est premier, False sinon.
  """
  if n <= 1:
    return False
  for i in range(2, int(n**0.5) + 1):
    if n % i == 0:
      return False
  return True

